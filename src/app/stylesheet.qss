QToolButton#emptyToolButton {
    height: 1px;
}

QToolButton#inactiveToolButton:hover,
QToolButton#emptyToolButton:hover {
    background: transparent;
}

QToolButton#inactiveToolButton:pressed,
QToolButton#emptyToolButton:pressed {
    background: transparent;
}

QToolButton#inactiveToolButton:checked,
QToolButton#emptyToolButton:checked {
    background: transparent;
}

QToolTip {
    background: rgb(60, 60, 60);
    color: white;
    border: 1px solid black;
    padding: 0.1em;
}
