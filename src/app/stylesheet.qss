QWidget {
    background: rgb(60, 60, 60);
    color: white;
    outline: 0;
}

QWidget#transparentWidget {
    background: transparent;
}

QWidget#colorLabel {
   background: black;
   border: 1px solid black;
   padding-left: -1px;
}

QMainWindow::separator {
    background: rgb(30, 30, 30);
    width: 8px;
    height: 8px;
}

/*QCheckBox {
    border: 1px solid white;
}*/

QCheckBox::indicator, QGroupBox::indicator {
    border: 1px solid rgb(255, 255, 255);
    border-radius: 2px;
}

QCheckBox::indicator:checked, QGroupBox::indicator:checked {
    background: url(:/checkbox/Checkbox_checked_normal.png);
    background-position: center center;
    background-repeat: no-repeat;
    background-origin: border;
}

QCheckBox::indicator:checked:disabled, QGroupBox::indicator:checked:disabled {
    background: url(:/checkbox/Checkbox_checked_disabled.png);
    background-position: center center;
    background-repeat: no-repeat;
    background-origin: border;
}

QCheckBox::indicator:disabled, QGroupBox::indicator:disabled {
    border: 1px solid rgb(180, 180, 180);
}

QWidget#darkWidget {
    background: rgb(45, 45, 45);
}

QWidget#menuBarWidget {
    border-top: 0;
    border-bottom: 1px solid black;
    color: black;
    background-color: qlineargradient(x1:0, y1:0, x2:0, y2:1,
    stop:0 lightgray, stop:1 darkgray);
}

QComboBox {
    background-color: rgb(200, 200, 200);
    border: 1px solid black;
    border-radius: 5px;
/*  border-top-right-radius: 8px */
/*  border-bottom-right-radius: 8px */
    /*min-width: 6em;*/
    color: black;
    padding: 0px 5px 0px 5px;
}

QComboBox:on { /* shift the text when the popup opens */
    border-bottom-right-radius: 0px;
    border-bottom-left-radius: 0px;
}

QComboBox:disabled {
    background-color: rgb(180, 180, 180);
    border: 1px solid rgb(60, 60, 60);
    color: rgb(60, 60, 60);
}

QComboBox::drop-down {
    margin: -1 -1 -1 -1;
    background-color: rgb(50, 50, 50);
    width: 1em;

    border: 1px solid black;
    border-left-width: 1px;
    border-left-color: black;
    border-left-style: solid; /* just a single line */
    border-top-right-radius: 5px; /* same radius as the QComboBox */
    border-bottom-right-radius: 5px;
}

QComboBox::drop-down:hover {
    background-color: rgb(70, 70, 70);
}

QComboBox::drop-down:pressed {
    background-color: rgb(30, 30, 30);
}

QComboBox::drop-down:on {
    border-bottom-right-radius: 0px;
}

QAbstractItemView {
    background-color: rgb(255, 255, 255);
    color: black;
    border: 1px solid black;
    selection-background-color: rgb(200, 200, 200);
    selection-color: black;
}

QMenu {
    background-color: rgb(255, 255, 255);
    border: 1px solid black;
/*    margin-top: -1px;*/
/*    padding-top: -1px;*/
}

QMenu::item {
    padding: 2px 25px 2px 25px;
    background: transparent;
    color: black;
}

QMenu::item:selected {
    background-color: rgb(200, 200, 200);
    color: black;
}

QMenu::item:disabled {
    color: gray;
}

QMenuBar {
    color: black;
    border-bottom: 1px solid black;
    background-color: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                                      stop:0 lightgray, stop:1 darkgray);
}

QMenuBar#cornerMenuBar {
    background: transparent;
    border: 0;
    margin-bottom: -1px;
    padding: 0 4px;
}

QMenuBar::item {
    padding: 1px 4px;
    background: transparent;
}

QMenuBar#cornerMenuBar::item {
    padding: 1px 0px;
}

QMenuBar::item:selected { /* when selected using mouse or keyboard */
    background: #a8a8a8;
}

QMenuBar::item:disabled {
    color: rgb(60, 60, 60);
}

QMenuBar::item:pressed {
    background: #888888;
}

QComboBox QScrollBar {
    border-top: 0px solid black;
    border-bottom: 0px solid black;
    background-color: rgb(170, 170, 170);
}

QComboBox:focus {
    color: black;
    background-color: rgb(255, 255, 255);
    padding: 0px 5px 0px 5px;
}

QComboBox QScrollBar::sub-line {
    border-right: 0px solid black;
    border-top: 0px solid black;
}

QComboBox QScrollBar::add-line {
    border-right: 0px solid black;
    border-bottom: 0px solid black;
}

QLabel {
    background: none;
}

QWidget:disabled {
    color: grey;
}

QLabel#dockLabel {
    border: 0;
    border-bottom: 1px solid black;
    padding-bottom: 1px;
    padding-top: 1px;
    color: black;
    background-color: qlineargradient(x1:0, y1:0, x2:0, y2:1,
    stop:0 lightgray, stop:1 darkgray);
}

QDockWidget {
    color: black;
}

QDockWidget::title {
    text-align: center;
    border: 0;
    border-bottom: 1px solid black;
    padding-bottom: 1px;
    padding-top: 1px;
    color: black;
    background-color: qlineargradient(x1:0, y1:0, x2:0, y2:1,
    stop:0 lightgray, stop:1 darkgray);
}

QLabel#blackTextLabel {
    color: black;
    background: transparent;
}
/*QMenuBar {
    color: white;
    background-color: rgb(50, 50, 50);
}*/

QTabBar {
    qproperty-drawBase: 0;
}

QTabBar::tab {
    color: white;
    border: 1px solid black;
    border-bottom: 1px transparent black;
    background-color: rgb(88, 88, 88);
    padding: 5px;
    min-width: 50px;
    border-top-left-radius: 2px;
    border-top-right-radius: 2px;
}

QTabBar::tab:selected {
    color: black;
    background-color: rgb(144, 144, 144);
    border: 1px solid black;
    border-bottom: 1px transparent black;
    border-top-left-radius: 2px;
    border-top-right-radius: 2px;
}

QTabBar::tab:disabled:selected {
    color: darkgray;
    background-color: rgb(120, 120, 120);
    border: 1px solid rgb(60, 60, 60);
}

QTabBar::tab:disabled:!selected {
    background-color: rgb(100, 100, 100);
    border: 1px solid rgb(60, 60, 60);
    color: gray;
}

QTabBar::tab:hover:!selected {
    background-color: rgb(100, 100, 100);
}

QTabWidget {
    border: 0px transparent black;
}

QTabWidget::pane {
    border: 1px solid #76797C;
    padding: 5px;
    margin: 0px;
}

QPushButton {
    qproperty-iconSize: 17px;
    background: qlineargradient(x1:0, y1:0,
                                x2:0, y2:1,
        stop:0 rgb(200, 200, 200),
        stop:0.5 rgb(188, 188, 188));
    border: 1px solid black;
    border-radius: 5px;
    color: black;
    outline: none;
    padding: 2px 2px 2px 2px
}

QPushButton#dialogButton {
    width: 4.5em;
}

QPushButton#iconButton {
    background: transparent;
    border: 0;
}

QPushButton#addCanvasButton {
    border-top-right-radius: 0;
    border-bottom-right-radius: 0;
    border-right: 0px solid black;
    border-left: 0px solid black;
}

QPushButton#addCanvasButton,
QPushButton#removeCanvasButton {
    padding: 0;
    border-top-left-radius: 0;
    border-bottom-left-radius: 0;
}

QComboBox#currentLayoutComboBox {
    border-top-right-radius: 0;
    border-bottom-right-radius: 0;
}

QComboBox#currentLayoutComboBox::drop-down {
    border-radius: 0;
    border-top-left-radius: 5px; /* same radius as the QComboBox */
    border-bottom-left-radius: 5px;
}

QPushButton:hover {
    background: qlineargradient(x1:0, y1:0,
                                x2:0, y2:1,
        stop:0 rgb(215, 215, 215),
        stop:0.8 rgb(188, 188, 188));
}

QPushButton:pressed {
    background: qlineargradient(x1:0, y1:0,
                                x2:0, y2:1,
        stop:0 rgb(235, 235, 235),
        stop:0.8 rgb(208, 208, 208));
}

QPushButton:hover:checked {
background: qlineargradient(x1:0, y1:0,
                            x2:0, y2:1,
    stop:0 rgb(80, 80, 80),
    stop:0.8 rgb(50, 50, 50));
}

QPushButton:checked {
    background: qlineargradient(x1:0, y1:0,
                                x2:0, y2:1,
        stop:0 rgb(70, 70, 70),
        stop:0.5 rgb(50, 50, 50));
    color: white;
}

QPushButton:disabled {
    background: qlineargradient(x1:0, y1:0,
                                x2:0, y2:1,
        stop:0 rgb(160, 160, 160),
        stop:0.5 rgb(148, 148, 148));
    color: rgb(65, 65, 65);
}

QPushButton#darkButton {
    color: white;
    background: qlineargradient(x1:0, y1:0,
                                x2:0, y2:1,
        stop:0 rgb(125, 125, 125),
        stop:0.5 rgb(60, 60, 60));
}

QPushButton#darkButton:disabled {
    background: qlineargradient(x1:0, y1:0,
                                x2:0, y2:1,
        stop:0 rgb(160, 160, 160),
        stop:0.5 rgb(148, 148, 148));
    color: rgb(200, 200, 200);
}

QPushButton#middleButton,
QPushButton#rightButton,
QPushButton#leftButton {
    color: white;
    background: qlineargradient(x1:0, y1:0,
                                x2:0, y2:1,
        stop:0 rgb(70, 70, 70),
        stop:0.5 rgb(50, 50, 50));
}

QPushButton#middleButton:disabled,
QPushButton#rightButton:disabled,
QPushButton#leftButton:disabled {
    color: rgb(200, 200, 200)   ;
    background: qlineargradient(x1:0, y1:0,
                                x2:0, y2:1,
        stop:0 rgb(110, 110, 110),
        stop:0.5 rgb(80, 80, 80));
}

QPushButton#middleButton {
    border-radius: 0px;
    border-right: 0;
}

QPushButton#rightButton {
    border-bottom-left-radius: 0px;
    border-top-left-radius: 0px;
}

QPushButton#leftButton {
    border-bottom-right-radius: 0px;
    border-top-right-radius: 0px;
    border-right: 0;
}

QPushButton#middleButton:checked,
QPushButton#rightButton:checked,
QPushButton#leftButton:checked {
    background: qlineargradient(x1:0, y1:0,
                                x2:0, y2:1,
        stop:0 rgb(85, 175, 235),
        stop:0.5 rgb(60, 150, 200));
    color: black;
}

QPushButton#middleButton:checked:disabled,
QPushButton#rightButton:checked:disabled,
QPushButton#leftButton:checked:disabled {
    background: qlineargradient(x1:0, y1:0,
                                x2:0, y2:1,
        stop:0 rgb(85, 125, 180),
        stop:0.5 rgb(60, 120, 160));
    color: rgb(60, 60, 60);
}

QPushButton#renderSettings {
    color: lightblue;
    background: transparent;
    border: 0;
}

QPushButton#renderSettings:hover {
    text-decoration: underline;
}

QToolBar {
    spacing: 4;
    background: qlineargradient(x1:0, y1:0,
                                x2:0, y2:1,
        stop:0.5 rgb(110, 110, 110),
        stop:1 rgb(90, 90, 90));
    border-bottom: 1px solid black;
}

QToolBar::separator {
    background: qlineargradient(x1:0, y1:0,
                                x2:0, y2:1,
        stop:0 rgba(0, 0, 0, 0),
        stop:0.5 black,
        stop:1 rgba(0, 0, 0, 0));
    width: 1px; /* when vertical */
    height: 1px; /* when horizontal */
    margin: 4px 4px 4px 4px
}

QScrollBar::add-page, QScrollBar::sub-page {
    background: none;
}

QScrollBar {
    border: 0;
    background-color: transparent;
}

QComboBox QScrollBar {
    background-color: rgb(50, 50, 50);
    border-right: 0px solid black;
}

QScrollBar::sub-line {
    border: 0;
    background-color: transparent;
}

QScrollBar::add-line {
    border: 0;
    background-color: transparent;
}

QScrollBar::add-line:hover, QScrollBar::sub-line:hover {
    background-color: rgb(70, 70, 70);
}

QScrollBar::add-line:pressed, QScrollBar::sub-line:pressed {
    background-color: rgb(31, 31, 31);
}

QScrollBar::handle {
    background: rgb(100, 100, 100);
}

QScrollBar::handle:hover {
    background: rgb(120, 120, 120);
}

QScrollBar::handle:pressed {
    background: rgb(140, 140, 140);
}

QScrollBar::handle:disabled  {
    background: transparent;
}

QScrollBar:vertical {
    margin: 20px 0 20px 0;
    max-width: 20px;
}
QScrollBar::handle:vertical {
    min-height: 18px;
}

QScrollBar::add-line:vertical {
    height: 19px;
    subcontrol-position: bottom;
    subcontrol-origin: margin;
}

QScrollBar::sub-line:vertical {
    height: 19px;
    subcontrol-position: top;
    subcontrol-origin: margin;
}

QScrollBar:horizontal  {
    margin: 0px 20px 0px 20px;
    max-height: 20px;
}

QScrollBar::handle:horizontal  {
    min-width: 18;
}

QScrollBar::add-line:horizontal  {

    width: 20px;
    subcontrol-position: left;
    subcontrol-origin: margin;
}

QScrollBar::sub-line:horizontal  {

    width: 20px;
    subcontrol-position: right;
    subcontrol-origin: margin;
}

QProgressBar {
    border: 1px solid black;
    color: transparent;
}

QProgressBar::chunk {
    background-color: green;
    width: 5px;
}

QProgressBar#hardwareUsage {
    border: 0;
    background-color: rgb(40, 40, 40);
}

QProgressBar#hardwareUsage::chunk {
    background-color: white;
    width: 5px;
    margin: 0.5px;
}

QToolButton {
    padding: 1px 4px;
    background: transparent;
    border-radius: 4px;
    color: black;
    margin-top: 10;
    border-bottom-right-radius: 0px;
    border-bottom-left-radius: 0px;
    padding-bottom: 9;
    margin-bottom: 1;
}

QToolButton:hover {
    background: rgba(0, 0, 0, 30);
}

QToolButton:pressed {
    background: rgba(0, 0, 0, 50);
}

QToolButton:checked {
    background: rgb(60, 60, 60);
    color: white;
}

